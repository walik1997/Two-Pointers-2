#leet code 80. Remove Duplicates from Sorted Array II
#time complexity - O(n)
#space complexity -O(1)
class Solution:
    def removeDuplicates(self, nums: List[int]) -> int:
        a=len(nums)
        slow=0
        count=1
        for i in range(1,a):
            if nums[i]==nums[i-1]:
                count+=1
            else:
                count=1
            if count <=2:
                slow +=1
                nums[slow]=nums[i]
        return slow+1

